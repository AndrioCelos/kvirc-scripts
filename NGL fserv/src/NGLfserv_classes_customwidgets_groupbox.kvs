alias(NGLfserv::classes::customwidgets::groupbox)
{
	class(NGLfserv::classes::customwidgets::groupbox,groupbox)
	{
		constructor(title,inside spacing,inside margin, column layout, strip)
		{
			$$->$setTitle($0)
			if ($1) $$->$setInsideSpacing($1)
			if ($2) $$->$setInsideMargin($2)
			if ($3){
				if (!$4) %strip=1
				else %strip=$4
				$$->$setLayoutOrientation(%strip,$3)
			}
		}
	}
}

