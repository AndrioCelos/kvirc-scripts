alias(NGLfserv::classes::customwidgets::button)
{
	class(NGLfserv::classes::customwidgets::button,button)
	{
		constructor(text)
		{
			$$->$setText($tr($0))
		}
		setImage(icon)
		{
			%img=%G_icons->$getIcon($0)
			if (!%img) $$->$button::setImage($0)
			else $$->$button::setImage(%img)
		}
	}
}

